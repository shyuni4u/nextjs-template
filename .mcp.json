{
  "mcpServers": {
    "supabase": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--read-only",
        "--project-ref={__project-id__}"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "{__supabase-access-token__}"
      }
    }
  }
}
